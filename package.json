{
  "name": "@hkngoc/rtk-query",
  "version": "1.6.1",
  "description": "Another way to use endpoint of redux-toolkit/rtk-query",
  "repository": {
    "type": "git",
    "url": "https://github.com/hkngoc/rtk-query"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "start": "tsc -p tsconfig.esm.json --watch",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-knobs": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/react": "^6.1.9",
    "@types/codemirror": "0.0.55",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/node": "^9.4.6",
    "@types/react": "^17.0.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^7.1.5",
    "cross-env": "^5.1.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "rimraf": "^2.6.1",
    "storybook-addon-jsx": "^7.3.4",
    "typescript": "4.0.3"
  }
}
